{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended",
    "prettier",
    "prettier/flowtype",
    "prettier/react"
  ],
  "globals": {
    "graphql": 0
  },
  "parser": "babel-eslint",
  "plugins": [
    "flowtype",
    "prettier"
  ],
  "rules": {
    "import/first": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/jsx-filename-extension": 0,
    "react/no-danger": 0,
    "react/prop-types": 0
  }
}
